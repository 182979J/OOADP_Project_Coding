  {{!-- <h2>Accordion Example</h2>
  <p><strong>Note:</strong> The <strong>data-parent</strong> attribute makes sure that all collapsible elements under the specified parent will be closed when one of the collapsible item is shown.</p> --}}
  <div class="panel-group" id="accordion">

    <div class="panel panel-default" style="width: 70em; margin-left: 150px;">

      <div class="panel-heading" style="background-color: moccasin ;"> {{!--navajowhite--}}
        <h4 class="panel-title" style="height: 20px; ">
          <a data-toggle="collapse" data-parent="#accordion" href="#collapse1">{{dateToday}}</a>
        </h4>
      </div>
      <div id="collapse1" class="panel-collapse collapse in">
