              <label>Item & quantity</label>
              {{!-- {{#each orderItem}} --}}
              <p id="p">{{ItemName}}</p>

              {{!-- {{/each}} --}}
            </div>
            <br><br>

            {{!-- </div> --}}
          </div>







        </div>
          {{/each}}

          {{else}}
          <p style="text-align: center; font-weight: bold;">There are no deliveries on {{dateToday}}</p>
          {{/if}}
      </div>
    </div>


